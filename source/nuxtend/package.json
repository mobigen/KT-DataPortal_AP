{
  "name": "dataportal-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=local nuxt dev --dotenv ./config/.env.local",
    "build": "cross-env NODE_ENV=local nuxt build --dotenv ./config/.env.local",
    "start": "cross-env NODE_ENV=local nuxt start --dotenv ./config/.env.local",
    "generate": "cross-env NODE_ENV=local nuxt generate --dotenv ./config/.env.local",
    "generate:dev": "cross-env NODE_ENV=dev nuxt generate --dotenv ./config/.env.dev",
    "generate:prod": "cross-env NODE_ENV=prod nuxt generate --dotenv ./config/.env.prod"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/i18n": "^7.2.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/storybook": "^4.3.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-controls": "^6.4.22",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.19.3",
    "cross-env": "^7.0.3",
    "moment": "^2.26.0",
    "nuxt": "^2.15.8",
    "nuxt-build-optimisations": "^1.0.7",
    "rsajs": "2.0.0",
    "vue": "^2.6.14",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.15.0",
    "vue-loading-overlay": "^3.2.0",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuex-persistedstate": "^3.2.1",
    "webpack": "^4.46.0"
