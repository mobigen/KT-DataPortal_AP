{
  "name": "nuxtend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "nuxt --port 8000 --mode developement",
    "build": "nuxt build --mode production",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@yzfe/svgicon": "~1.1.0",
    "@yzfe/vue-svgicon": "~4.0.2",
    "vue": "^2.6.14",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.15.0",
    "vue-loading-overlay": "^3.2.0",
    "axios": "^0.21.1",
    "nuxt": "^2.15.8",
    "sass": "^1.50.0",
    "sass-loader": "^10.2.1",
    "vuex": "^3.1.3",
    "core-js": "^3.19.3",
    "webpack": "^4.46.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
